var compatibleProperties = "sds.double";
var description = "Logic Component Example";
var parameters = new Array("Arbitrary PV1", "Arbitrary PV2", "Arbitrary PV3");

function execute(args) {
	var arg1 = args[0];
	var arg2 = args[1];
	var arg3 = args[2];

	if (arg1 < 0) {
		arg1 = arg1 * -1;
	}

	if (arg2 < 0) {
		arg2 = arg2 * -1;
	}

	if (arg3 < 0) {
		arg3 = arg3 * -1;
	}
	
	var sum = arg1 + arg2 + arg3;

	return sum / 3;
}
